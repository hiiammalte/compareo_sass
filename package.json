{
  "name": "compareo_sass",
  "version": "1.0.0",
  "description": "compareo app design using scss",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-app": "sass --watch scss/main.scss src/assets/css/main.css",
    "build-app": "sass scss/main.scss src/assets/css/main.css",
    "compress-app": "sass scss/main.scss src/assets/css/main-min.css --style=compressed",
    "watch-public": "sass --watch scss/pages/public.scss src/assets/css/public.css",
    "build-public": "sass scss/pages/public.scss src/assets/css/public.css",
    "compress-public": "sass scss/pages/public.scss src/assets/css/public-min.css --style=compressed"
  },
  "author": "Malte Fesser",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "sass": "^1.32.8"
  }
}
